    "@types/jest": "^27.0.2",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3",
    "typescript-tslint-plugin": "^1.0.1"
  },
  "dependencies": {
    "node-fetch": "^3.0.0"
  },
  "files": [
    "lib/**/*"
